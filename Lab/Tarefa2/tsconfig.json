{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": [
      "src/**/*.ts",
      "test/**/*.ts" // Seus arquivos de teste em TypeScript
    ],
    "exclude": [
      "node_modules"
    ]
  }
  